{
  "expo": {
    "name": "bahia-driver",
    "slug": "bahia-driver",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-bahia-driver-azul.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo-bahia-driver-branco.png",
      "resizeMode": "contain",
      "backgroundColor": "#005A9C"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fabio.fbn.bahiadriver",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-bahia-driver-azul.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.fabio.fbn.bahiadriver"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-maps"
    ],
    "extra": {
      "eas": {
        "projectId": "ed0c61a6-fcbd-4d19-9670-1ca249835b3c"
      }
    }
  }
}
